		{ "fromDir": "/resources/css/legacy/", "toDir": "/generated-resources/css/legacy/" },

		{ "fromDir": "/resources/img/", "toDir": "/generated-resources/img/" },
		{ "fromDir": "/resources/img/modig/", "toDir": "/generated-resources/img/modig/" },
		{ "fromDir": "/resources/img/legacy/", "toDir": "/generated-resources/img/legacy/" }
	],

	"copyFiles"  : [
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/bootstrap/", "file": "console.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/bootstrap/", "file": "html5.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/bootstrap/", "file": "html5.min.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/bootstrap/", "file": "underscore.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/bootstrap/", "file": "underscore-min.js" },

		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/modig/", "file": "console.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/modig/", "file": "html5.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/modig/", "file": "html5.min.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/modig/", "file": "underscore.js" },
		{ "fromDir": "/resources/js/", "toDir": "/generated-resources/js/modig/", "file": "underscore-min.js" }
	],

	"concatFiles": [
		{ "fromDir": "/resources/js/bootstrap/", "toFile": "/generated-resources/js/bootstrap/bootstrap.js",
			"files": "fs.readdirSync(baseFolder + '/resources/js/bootstrap/')" },

		{ "fromDir": "/resources/js/modig/", "toFile": "/generated-resources/js/modig/modig.js",
			"files": ["transitions.js", "felles.js", "dropdown.js", "tooltip.js", "AjaxLoader.js"] },

        { "fromDir": "/resources/js/jquery/", "toFile": "/generated-resources/js/enonic/navno/jquery-tmp.js",
            "files": ["jquery-1.11.1.js"] },

		{ "fromDir": "/resources/js/enonic/navno/", "toFile": "/generated-resources/js/enonic/navno/navno-tmp.js",
			"files": ["nav-global.js", "nav-carousel.js", "nav-content.js", "nav-accordion.js",
                      "libs/matchmedia.js", "libs/megamenu/jquery-accessibleMegaMenu.js",
                      "nav-klage-ris-ros.js"] },

        { "fromDir": "/generated-resources/js/enonic/navno/", "toFile": "/generated-resources/js/enonic/navno/navno-concat.js",
            "files": ["jquery-tmp.js", "navno-tmp.js"] },

		{ "fromDir": "/resources/less/modig/", "toFile": "/generated-resources/less/modig/modig.less",
			"files": ["reset.less", "variables.less", "mixins.less", "modus.less", "typografi.less", "felles.less",
					  "knapper.less", "contrast.less", "dropdowns.less", "skjema.less", "tooltip.less", "utilities.less",
					  "panel.less", "footer.less", "ekstern.less"] }
	],

	"uglify": [
		{ "fromFile": "/generated-resources/js/bootstrap/bootstrap.js", "toFile": "/generated-resources/js/bootstrap/bootstrap.min.js" },
		{ "fromFile": "/generated-resources/js/modig/modig.js", "toFile": "/generated-resources/js/modig/modig.min.js" },
		{ "fromFile": "/generated-resources/js/enonic/navno/navno-concat.js", "toFile": "/generated-resources/js/enonic/navno/navno.min.js" },
		{ "fromFile": "/generated-resources/js/legacy/felles.js", "toFile": "/generated-resources/js/legacy/felles.min.js" },

        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/annenpost.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/annenpost.min.js"
        },
        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/elektroniskinnsending.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/elektroniskinnsending.min.js"
