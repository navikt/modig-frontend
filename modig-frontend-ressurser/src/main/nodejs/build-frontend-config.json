        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/ettersendelsesveileder.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/ettersendelsesveileder.min.js"
        },
        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/ettersendelsesveilederarbeidsgiver.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/ettersendelsesveilederarbeidsgiver.min.js"
        },
        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/felles.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/felles.min.js"
        },
        {
            "fromFile": "/generated-resources/js/enonic/skjemaveileder/skjemavalg.js",
            "toFile": "/generated-resources/js/enonic/skjemaveileder/skjemavalg.min.js"
        }
	],

	"runLess": [
		{ "fromDir": "/resources/less/bootstrap/", "mainFile": "/resources/less/bootstrap/bootstrap.less",
			"builtFile": "/generated-resources/css/bootstrap/bootstrap.css" },

		{ "fromDir": "/generated-resources/less/modig/", "mainFile": "/generated-resources/less/modig/modig.less",
			"builtFile": "/generated-resources/css/modig/modig.css" },

		{ "fromDir": "/generated-resources/less/enonic/", "mainFile": "/resources/less/enonic/nav-old-ie.less",
			"builtFile": "/generated-resources/css/enonic/nav-old-ie.css" },
		{ "fromDir": "/generated-resources/less/enonic/", "mainFile": "/resources/less/enonic/nav-responsive.less",
			"builtFile": "/generated-resources/css/enonic/nav-responsive.css" },

		{ "fromDir": "/generated-resources/less/legacy/", "mainFile": "/resources/less/legacy/nav-standard.less",
			"builtFile": "/generated-resources/css/legacy/nav-standard.css" }
	],

    "remove": [
        { "fileName": "/generated-resources/js/enonic/navno/navno-tmp.js" },
        { "fileName": "/generated-resources/js/enonic/navno/jquery-tmp.js" }
    ]
