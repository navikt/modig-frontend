        "js/enonic/skjemaveileder/",
		"js/legacy/",
		"less/",
		"less/bootstrap/",
		"less/modig/",
		"less/enonic/", "less/enonic/includes/",
		"less/legacy/"
	],

	"copyFolders"  : [
		{ "fromDir": "/resources/js/bootstrap/", "toDir": "/generated-resources/js/bootstrap/" },
		{ "fromDir": "/resources/js/jquery/", "toDir": "/generated-resources/js/bootstrap/jquery/" },

		{ "fromDir": "/resources/js/modig/", "toDir": "/generated-resources/js/modig/" },
		{ "fromDir": "/resources/js/jquery/", "toDir": "/generated-resources/js/modig/jquery/" },

		{ "fromDir": "/resources/js/enonic/navno/", "toDir": "/generated-resources/js/enonic/navno/" },
		{ "fromDir": "/resources/js/enonic/skjemaveileder/", "toDir": "/generated-resources/js/enonic/skjemaveileder/" },
        { "fromDir": "/resources/js/enonic/navno/libs/", "toDir": "/generated-resources/js/enonic/navno/libs/" },
        { "fromDir": "/resources/js/enonic/navno/libs/megamenu/", "toDir": "/generated-resources/js/enonic/navno/libs/megamenu/" },
        { "fromDir": "/resources/js/jquery/", "toDir": "/generated-resources/js/enonic/navno/libs/jquery/" },

		{ "fromDir": "/resources/less/bootstrap/", "toDir": "/generated-resources/less/bootstrap/" },
		{ "fromDir": "/resources/js/legacy/", "toDir": "/generated-resources/js/legacy/" },

		{ "fromDir": "/resources/less/modig/", "toDir": "/generated-resources/less/modig/" },

